{
  "name": "@awsm/icons",
  "version": "0.0.2",
  "author": "Evgeny Khoroshilov (https://github.com/XOP)",
  "license": "MIT",
  "private": true,
  "type": "module",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.esm.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js"
    },
    "./lib/*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.esm.js",
      "require": "./dist/*.cjs.js"
    },
    "./svg/*": "./svg/*"
  },
  "files": [
    "dist",
    "lib/icons",
    "svg"
  ],
  "scripts": {
    "lint": "eslint --ext .js,.cjs,.ts,.cts,.tsx scripts/ stories/",
    "gen:clean": "rimraf ./lib/icons && mkdirp ./lib/icons",
    "gen": "pnpm gen:clean && tsx ./scripts/generateIcons.cts",
    "build:clean": "rimraf ./dist",
    "build:icons": "pnpm build:clean && tsup-node && tsc --project ./tsconfig.build.json",
    "build": "pnpm gen && pnpm build:icons"
  },
  "devDependencies": {
    "@awsm/tokens": "workspace:*",
    "@awsm/tsconfig": "workspace:*",
    "@storybook/blocks": "^7.6.1",
    "@svgr/core": "^8.1.0",
    "@svgr/plugin-jsx": "^8.1.0",
    "@svgr/plugin-svgo": "^8.1.0",
    "@types/react": "^18.2.39",
    "@vanilla-extract/css": "^1.14.0",
    "eslint": "^8.54.0",
    "eslint-config-awsm": "workspace:*",
    "react": "^18.2.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.2"
  }
}
