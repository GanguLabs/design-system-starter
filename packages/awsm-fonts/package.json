{
  "name": "@awsm/fonts",
  "version": "0.0.3",
  "author": "Evgeny Khoroshilov (https://github.com/XOP)",
  "license": "MIT",
  "private": true,
  "type": "module",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.esm.js",
  "types": "./dist/entries/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/entries/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js"
    },
    "./css/*": "./dist/css/*"
  },
  "files": [
    "dist/css",
    "dist/entries",
    "dist/src",
    "dist/index.*",
    "fonts",
    "src"
  ],
  "scripts": {
    "lint": "eslint --ext .js,.cjs,.ts,.cts,.tsx src/ entries/ stories/",
    "format": "biome format --write ./entries ./src ./stories",
    "build": "vite build && tsc --project tsconfig.build.json"
  },
  "devDependencies": {
    "@awsm/tokens": "workspace:*",
    "@awsm/tsconfig": "workspace:*",
    "@storybook/blocks": "^8.0.4",
    "@types/react": "^18.2.74",
    "@vitejs/plugin-react": "^4.2.0",
    "eslint": "^8.54.0",
    "eslint-config-awsm": "workspace:*",
    "react": "^18.2.0",
    "typescript": "^5.4.3",
    "vite": "^5.2.7"
  }
}
